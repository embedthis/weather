/*
    ai.me -- MakeMe File for the AI App

    MakeMe files are used by EmbedThis to generate Makefiles
 */

Me.load({
    targets: {
        ai: {
            configurable: true,
            ifdef:        [ 'apps' ],
            depends:      [ 'libai' ],
        },
        libai: {
            ifdef: [ 'ai' ],
            type: 'lib',
            path: '${BIN}/libapp${ARLIB}',
            sources: [ 'src/*.c' ],
        },
    },
})
