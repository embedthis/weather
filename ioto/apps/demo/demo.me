/*
    demo.me -- MakeMe File for the Demo App

    MakeMe files are used by EmbedThis to generate Makefiles
 */

Me.load({
    targets: {
        demo: {
            configurable: true,
            ifdef:        [ 'apps' ],
            depends:      [ 'libdemo' ],
        },
        libdemo: {
            ifdef: [ 'demo' ],
            type: 'lib',
            path: '${BIN}/libapp${ARLIB}',
            sources: [ 'src/*.c' ],
        },
    },
})
