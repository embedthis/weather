{
    version: '1.0.0',
    profile: 'dev',
    app: 'unit',
    directories: {
        log: '.',
        state: 'state',
    },
    optimize: 'debug',
    log: {
        path: 'stdout',
        format: '%S: %M',
        types: 'error,info',
        sources: 'all,!mbedtls',
        show: '',
    },
    database: {
        path: '@db/device.db',
        schema: '@config/schema.json5',
        maxJournalSize: '50k',
        maxJournalAge: '15secs',
        maxSyncSize: '10k',
        service: '1hr',
    },
    limits: {
        stack: '128k',
    },
    mqtt: {
        authority: '@certs/aws.crt',
        schedule: '* * * * *',
        delay: 15,
        timeout: 0,
    },
    services: {
        ai: false,
        database: true,
        demo: false,
        keys: false,
        logs: false,
        mqtt: true,
        provision: true,
        register: true,
        serialize: 'auto',
        shadow: false,
        sync: true,
        update: false,
        url: true,
        web: true,
    },
    tls: {
        authority: '@certs/roots.crt',
        certificate: '@certs/test.crt',
        key: '@certs/test.key',
    },
    update: {
        schedule: '* * * * *',
        period: '24 hr',
        jitter: 0,
        throttle: 0,
        apply: '* * * * *',
    }
}